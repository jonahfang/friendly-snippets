{
    "ExtJS.func": {
        "prefix": "func",
        "body": ["${1}: function () {", "    // $0", "},"]
    },
    "style": {
        "prefix": "style",
        "body": ["style={{$0}}"]
    },
    "onClick": {
        "prefix": "onc",
        "body": ["onClick={$0}"]
    },
    "arrow-function-0": {
        "prefix": "fn-const=>",
        "body": ["const ${1} = () => {", "    $0", "}"]
    },
    "arrow-function-1": {
        "prefix": "fn-var",
        "body": ["var ${1:f} = function() {", "    $0", "}"]
    },
    "arrow-function-2": {
        "prefix": "fn-method",
        "body": ["${1:f} : function() {", "    $0", "}"]
    },
    "arrow-function-3": {
        "prefix": "fn-=>",
        "body": ["(${1}) => {$0}"]
    },
    "if-statement": {
        "prefix": "if-only",
        "body": ["if (${1}) {", "    $0", "}"]
    },
    "if-else-statement": {
        "prefix": "if-else",
        "body": ["if (${1}) {", "    $2", "} else {", "    $0", "}"]
    },
    "if-else-if-statement": {
        "prefix": "if-elif",
        "body": ["if (${1}) {", "    $2", "} else if (${3}) {", "    $0", "}"]
    },
    "default-state": {
        "prefix": "defstate",
        "body": ["const defaultState = {", "    $0", "}"]
    },
    "default-data": {
        "prefix": "defdata",
        "body": ["const defaultData = {", "    $0", "}"]
    },
    "use-set-state": {
        "prefix": "usestate",
        "body": ["const [${1:state}, set${1/(.*)/${1:/capitalize}/}] = useSetState(defaultState)"]
    },
    "use-ref": {
        "prefix": "useref",
        "body": ["const ${1} = useRef($0)"]
    },
    "use-this": {
        "prefix": "usethis",
        "body": ["const me = useThis()"]
    },
    "use-feedback": {
        "prefix": "usefb",
        "body": ["const [FeedbackCmp, ajaxIndicator, topMsg, msgDialog] = useFeedback()"]
    },
    "use-defer-callback": {
        "prefix": "usedefercall",
        "body": ["const deferCallback = useDeferCallback()", "", "deferCallback(() => {", "    ${1:popRef}.current.show()", "})"]
    },
    "map-loop": {
        "prefix": "maploop",
        "body": ["{${1:items}.map((${2:item}) => (", "    $0", "))}"]
    },
    "for-loop": {
        "prefix": "for-var",
        "body": ["for (var i = 0; i < ${1:rows}.length; i++) {", "    var ${2:item} = ${1}[i]", "    $0", "}"]
    },
    "forEach-loop": {
        "prefix": "for-each",
        "body": ["${1:rows}.forEach((${2:row}) => {", "    $0", "})"]
    },
    "object-separator": {
        "prefix": "sep",
        "body": ["}, {"]
    },
    "console-log": {
        "prefix": "cons",
        "body": ["console.log($0)"]
    },
    "convert-function": {
        "prefix": "cvt-rec",
        "body": ["convert : function(${1:v}, ${2:rec}) {", "    $0", "}"]
    },
    "convert-function-2": {
        "prefix": "cvt-v",
        "body": ["convert : function(${1:vc}) {", "    $0", "}"]
    },
    "me-this": {
        "prefix": "methis",
        "body": ["var me = this"]
    },
    "my-id": {
        "prefix": "myid",
        "body": ["id : me.myId('${1:list}')"]
    }
}
